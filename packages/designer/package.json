    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
    "@toast-ui/react-editor": "^3.2.3",
    "ahooks": "^3.8.5",
    "allotment": "^1.20.3",
    "antd": "5.25.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.12.1",
